{
    "manifest_version": 2,
    "name": "timetracker",
    "description": "Productivity tool to save time that you waste in social media",
    "short_name": "timetracker",
    "version": "1.0",
    "browser_action": {
      "default_popup": "index.html"
    },
    "background": {
      "scripts": [
          "js/background.js"
        ]
      },
    "permissions": [
      "tabs",
      "storage"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }